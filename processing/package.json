{
  "name": "wagerr-bridge-processing",
  "version": "1.0.0",
  "description": "The processing backend for the wagerr bridge",
  "main": "process.js",
  "private": true,
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha -r esm --recursive --timeout 10000",
    "autoSwap": "cross-env NODE_ENV=production node -r esm process.js --autoSwap",
    "swap": "cross-env NODE_ENV=production node -r esm process.js --swap",
    "sweep": "cross-env NODE_ENV=production node -r esm process.js --sweep",
    "checkBalance": "cross-env NODE_ENV=production node -r esm process.js --check",
    "printInvalid": "cross-env NODE_ENV=production node -r esm process.js --printInvalid",
    "preinstall": "npm link ../bridge-core"
  },
  "author": "Wagerr Network",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.0",
    "bridge-core": "file:../bridge-core",
    "chalk": "^2.4.2",
    "commander": "^2.20.0",
    "config": "^3.1.0",
    "cross-env": "^5.2.0",
    "decimal.js": "^10.2.0",
    "esm": "^3.2.25",
    "lowdb": "^1.0.0",
    "strip-ansi": "^5.2.0",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.10.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3",
    "mocha": "^6.1.4",
    "sinon": "^7.3.2"
  }
}
